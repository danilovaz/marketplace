<h1>Listing order_templates</h1>

<table>
  <thead>
    <tr>
      <th>Template</th>
      <th>Order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_templates.each do |order_template| %>
      <tr>
        <td><%= order_template.template %></td>
        <td><%= order_template.order %></td>
        <td><%= link_to 'Show', order_template %></td>
        <td><%= link_to 'Edit', edit_order_template_path(order_template) %></td>
        <td><%= link_to 'Destroy', order_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order template', new_order_template_path %>
